{
  "name": "szeremi",
  "version": "1.0.0",
  "description": "Attila Szeremi's portfolio website",
  "main": "index.js",
  "scripts": {
    "start": "node --harmony ./dist/server.js",
    "build-server": "node ./node_modules/webpack/bin/webpack.js --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "node ./node_modules/webpack/bin/webpack.js --colors --display-error-details --config configs/webpack.client.js",
    "build": "node ./node_modules/concurrently/src/main.js \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "node ./node_modules/webpack/bin/webpack.js --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node ./node_modules/wait-run/bin/wait-run.js --pattern \"dist/*.js\" \"node --harmony ./dist/server.js\"",
    "watch-client": "node hot.js",
    "watch-start": "node ./node_modules/concurrently/src/main.js \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\"",
    "watch": "node ./node_modules/concurrently/src/main.js \"npm run watch-server\" \"npm run watch-client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amcsi/szeremi.git"
  },
  "author": "Attila Szeremi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amcsi/szeremi/issues"
  },
  "homepage": "https://github.com/amcsi/szeremi#readme",
  "dependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bootstrap": "^3.3.6",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "history": "^1.17.0",
    "i18next": "^2.0.9",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "path": "^0.12.7",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.3",
    "react-i18next": "0.0.4",
    "react-redux": "^4.0.5",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-simple-router": "^1.0.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "wait-run": "^1.1.0",
    "webpack": "^1.12.12"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "eslint": "^1.10.2",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.1",
    "net": "^1.0.2",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "source-map-index-generator": "^0.1.2",
    "source-map-support": "^0.4.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
